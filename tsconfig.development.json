{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "./dist/development",                   /* Specify an output folder for all emitted files. */
    "sourceMap": true,                                /* Create source map files for emitted JavaScript files. */
    "noErrorTruncation": true,                        /* Disable truncating types in error messages. */
    "traceResolution": true,                          /* Enable tracing of the name resolution process. Requires TypeScript version 2.0 or later. */
    "listFiles": true,                                /* Print all of the files read during the compilation. */
    "diagnostics": true,                              /* Output compiler performance information after building. */
    "removeComments": false,                          /* Disable emitting comments. */
    "stripInternal": false,                           /* Disable emitting declarations that have '@internal' in their JSDoc comments. */
  }
}
