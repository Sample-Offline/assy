{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "./dist/production",
    "sourceMap": true,
    "noErrorTruncation": false,
    "traceResolution": false,
    "listFiles": false,
    "diagnostics": true,
    "removeComments": true,
    "stripInternal": true,                            /* Disable emitting declarations that have '@internal' in their JSDoc comments. */
  }
}
